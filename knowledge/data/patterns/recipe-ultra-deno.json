{
  "patternId": "recipe-ultra-deno",
  "name": "Ultra Deno",
  "description": "Recipe Ultra Deno application",
  "framework": "Recipe Ultra Deno",
  "language": "TypeScript",
  "tags": [
    "deno"
  ],
  "services": [
    {
      "enableSubdomainAccess": true,
      "hostname": "app",
      "type": "deno@1"
    }
  ],
  "zeropsYml": {
    "zerops": [
      {
        "build": {
          "base": "deno@1",
          "buildCommands": [
            "deno task build"
          ],
          "deployFiles": [
            ".ultra/~"
          ]
        },
        "run": {
          "base": "deno@1",
          "ports": [
            {
              "httpSupport": true,
              "port": 8000
            }
          ],
          "start": "deno task start"
        },
        "setup": "app"
      }
    ]
  },
  "sourceRecipe": "recipe-ultra-deno-main",
  "deploymentType": "local"
}