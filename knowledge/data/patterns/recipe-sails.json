{
  "patternId": "recipe-sails",
  "name": "Sails",
  "description": "Recipe Sails application",
  "framework": "Recipe Sails",
  "language": "Unknown",
  "tags": [],
  "services": [
    {
      "enableSubdomainAccess": true,
      "hostname": "app",
      "type": "nodejs@20"
    }
  ],
  "zeropsYml": {
    "zerops": [
      {
        "build": {
          "base": "nodejs@20",
          "buildCommands": [
            "pnpm i",
            "pnpm build"
          ],
          "deployFiles": [
            "/"
          ]
        },
        "run": {
          "base": "nodejs@20",
          "ports": [
            {
              "httpSupport": true,
              "port": 1337
            }
          ],
          "start": "node app.js"
        },
        "setup": "app"
      }
    ]
  },
  "sourceRecipe": "recipe-sails-main",
  "deploymentType": "local"
}