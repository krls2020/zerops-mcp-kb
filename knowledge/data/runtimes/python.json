{
  "runtime": "python",
  "displayName": "Python",
  "versions": {
    "available": ["3.12", "3.11", "3.10"],
    "default": "3.12"
  },
  "buildConfig": {
    "buildCommands": ["pip install -r requirements.txt"],
    "deployFiles": ["."],
    "addToRunPrepare": ["requirements.txt"]
